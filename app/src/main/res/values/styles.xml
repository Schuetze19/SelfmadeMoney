<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Profil theme -->
    <style name="ProfilTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/profilPrimaryColor</item>
        <item name="colorPrimaryDark">@color/profilPrimaryDarkColor</item>
        <item name="colorAccent">@color/profilAccentColor</item>
    </style>

</resources>
